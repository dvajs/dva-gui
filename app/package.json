{
  "name": "dva-gui",
  "version": "0.1.0",
  "description": "A gui tool for dva",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently --kill-others \"cross-env env=dev electron ./main.development.js\"",
    "build": "cross-env env=env webpack",
    "start": "cross-env env=pro electron ./",
    "lint": "eslint app",
    "test": "npm run lint",
    "clean": "rm -rf ./release",
    "package": "npm run clean && cross-env env=pro node ./scripts/package.js"
  },
  "dependencies": {
    "devtron": "^1.4.0",
    "dva-ast": "^0.3.1",
    "electron": "^1.3.4",
    "electron-debug": "^1.0.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "electron-packager": "^8.1.0",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "~4.3.0",
    "json-loader": "^0.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvajs/dva-gui"
  },
  "homepage": "https://github.com/dvajs/dva-gui",
  "keywords": [
    "dva",
    "gui"
  ],
  "author": "ChenCheng <sorrycc@gmail.com>",
  "license": "MIT",
  "theme": "./antd.config.js"
}